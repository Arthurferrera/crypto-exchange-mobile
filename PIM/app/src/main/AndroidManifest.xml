<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".telaSaque"></activity>
        <activity android:name=".telaAportes" />
        <activity android:name=".telaInvestir" />
        <activity android:name=".telaCarteira" />
        <activity android:name=".telaAlteraDados" />
        <activity android:name=".telaMeusDados" />
        <activity
            android:name=".telaCadastro"
            android:screenOrientation="portrait" />
        <activity android:name=".telaHome" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>